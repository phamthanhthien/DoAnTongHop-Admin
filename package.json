{
  "name": "elearning",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "backend": "nodemon -q -w src-backend --signal SIGTERM src-backend",
    "pret": "eslint --ignore-path=.gitignore --fix . && prettier --ignore-path=.gitignore --loglevel=silent --write \"**/*.{html,scss,json,md}\"",
    "pret-": "yarn pret && yarn pret-objc && yarn pret-java && yarn pret-xml && yarn pret-img",
    "pret-objc": "yarn git-ls | egrep -h '.(h|m)$' | xargs clang-format -i -style=file",
    "pret-java": "yarn git-ls | egrep -h '.(java)$' | xargs google-java-format -i",
    "pret-xml": "yarn git-ls | egrep -h '.(xml|xib|xccheme|xcworkspacedata|plist)$' | xargs -L1 bash -c 'xmllint --format --output $0 $0'",
    "pret-img": "yarn git-ls | egrep -h '.(png|jpg|gif|ico)$' | xargs -L1 bash -c 'imagemin $0 --out-dir $(dirname $0)'",
    "git-ls": "bash -c 'comm -3 <(git ls-files) <(git ls-files -d)'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@devexpress/dx-react-chart": "^2.6.3",
    "@devexpress/dx-react-chart-material-ui": "^2.6.3",
    "@devexpress/dx-react-core": "^2.6.2",
    "@devexpress/dx-react-grid": "^2.6.2",
    "@devexpress/dx-react-grid-material-ui": "^2.6.2",
    "@devexpress/dx-react-scheduler": "^2.6.2",
    "@devexpress/dx-react-scheduler-material-ui": "^2.6.2",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "ansi-colors": "^4.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.2",
    "base-x": "3.0.7",
    "classnames": "^2.2.6",
    "clsx": "^1.1.0",
    "colors": "1.4.0",
    "cookie": "0.4.0",
    "d3-ease": "^1.0.6",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "14.5.8",
    "graphql-iso-date": "3.6.1",
    "graphql-middleware": "4.0.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "4.0.6",
    "graphql-upload": "8.1.0",
    "history": "^4.10.1",
    "immer": "^5.0.1",
    "join-monster": "2.1.1",
    "join-monster-graphql-tools-adapter": "0.0.3",
    "lodash": "4.17.15",
    "material-table": "^1.57.2",
    "merge-graphql-schemas": "1.7.3",
    "moment": "^2.26.0",
    "mysql2": "2.0.1",
    "nodemailer": "6.3.1",
    "perfect-scrollbar": "^1.5.0",
    "pluralize": "8.0.0",
    "prop-types": "^15.7.2",
    "react-activity": "^1.2.2",
    "react-number-format": "^4.4.1",
    "react-router-dom": "^5.1.2",
    "require-directory": "2.1.1",
    "sequelize": "5.21.2",
    "socket.io": "2.3.0",
    "sql-formatter": "2.3.3",
    "stack-trace": "0.0.10",
    "validatorjs": "3.17.1"
  },
  "devDependencies": {
    "@mdi/js": "4.5.95",
    "jsc-android": "241213.1.0",
    "polished": "3.4.2",
    "react": "16.12.0",
    "react-art": "16.12.0",
    "react-device-detect": "1.9.10",
    "react-dom": "16.12.0",
    "react-google-maps": "9.4.5",
    "socket.io-client": "2.3.0"
  },
  "optionalDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-decorators": "7.7.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-class-properties": "7.2.0",
    "@babel/plugin-transform-flow-strip-types": "7.6.3",
    "@babel/plugin-transform-modules-commonjs": "7.7.0",
    "@babel/plugin-transform-react-jsx": "7.7.0",
    "@babel/register": "7.7.0",
    "@craco/craco": "5.6.2",
    "circular-dependency-plugin": "5.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.3",
    "faker": "4.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "node-sass": "4.13.0",
    "nodemon": "2.0.0",
    "prettier": "1.19.1",
    "react-scripts": "3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "prettier",
      "simple-import-sort",
      "sort-destructure-keys"
    ],
    "rules": {
      "sort-imports": "off",
      "simple-import-sort/sort": 2,
      "import/first": 2,
      "import/newline-after-import": 2,
      "import/no-duplicates": 2,
      "import/no-extraneous-dependencies": 2,
      "prettier/prettier": 2,
      "react/jsx-sort-props": 2,
      "react/sort-prop-types": 2,
      "sort-destructure-keys/sort-destructure-keys": 2,
      "quotes": [
        2,
        "backtick"
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "browserslist": "ie 10-11",
  "proxy": "http://localhost:3030"
}
